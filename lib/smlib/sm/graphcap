	:OW=\^PY\^-^M\^F\n\^IOP\^ISYNTAX00001\^IGV\^PW03\^V0\
		\^IMH0000008500\^IMV0000011000:\
	:RC@:\
	:ML=\^V(1$0)0($1)2($2)3($4)1($5)7($6)8($$:lt@=012456:\
	:MS=\^D:XY=(1%05d)\072(92-%05d:VS=\^U:DS=\^D:\
	:SY=echo 'file is $F':
